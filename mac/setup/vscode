ln -s $PWD/shared/vscode/settings.json $HOME/Library/Application\ Support/Code/User/settings.json
ln -s $PWD/shared/vscode/keybindings.json $HOME/Library/Application\ Support/Code/User/keybindings.json
ln -s $PWD/shared/vscode/snippets/ $HOME/Library/Application\ Support/Code/User/snippets

cat shared/vscode/extensions-*.txt | xargs -n 1 code --install-extension